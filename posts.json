---
layout: none
---
[
    {% for post in site.posts %}
    {
      "id"       : "{{ post.id }}",
      "title"    : "{{ post.title }}",
      "url"     : "{{ post.url }}",
      "date"     : {
         "day"   : "{{ post.date | date: "%d" }}",
         "month" : "{{ post.date | date: "%B" }}",
         "year"  : "{{ post.date | date: "%Y" }}"
      }
    }
    {% unless forloop.last %},{% endunless %}
    {% endfor %}
]